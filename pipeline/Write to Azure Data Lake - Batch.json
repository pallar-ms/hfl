{
	"name": "Write to Data Lake - Batch",
	"properties": {
		"activities": [
			{
				"name": "Save each input data",
				"type": "ForEach",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@pipeline().parameters.inputData",
						"type": "Expression"
					},
					"activities": [
						{
							"name": "Save to Data Lake",
							"type": "WebActivity",
							"dependsOn": [],
							"policy": {
								"timeout": "0.12:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"url": {
									"value": "@concat(pipeline().parameters.storageAccount,'/',pipeline().parameters.storageFolder,'/',item().resource.id)",
									"type": "Expression"
								},
								"method": "PUT",
								"headers": {
									"x-ms-blob-type": "BlockBlob",
									"x-ms-version": "2017-11-09"
								},
								"body": {
									"value": "@item().resource",
									"type": "Expression"
								},
								"authentication": {
									"type": "MSI",
									"resource": "https://storage.azure.com/"
								}
							}
						}
					]
				}
			}
		],
		"parameters": {
			"storageAccount": {
				"type": "string"
			},
			"storageFolder": {
				"type": "string"
			},
			"inputData": {
				"type": "array"
			}
		},
		"folder": {
			"name": "HFL/Load"
		},
		"annotations": [],
		"lastPublishTime": "2023-03-22T20:40:53Z"
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}